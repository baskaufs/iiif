        "within": {
          "@id": "http://8899a42f-c4bb-4682-b073-6aea961b5b4d",
          "@type": "sc:Manifest"
        }
      },
      "resource": [
        {
          "@type": "dctypes:Text",
          "format": "text/plain",
          "chars": "Artists often depict her in blue, a color usually associated with the Virgin Mary. Blue is also a sorrowful color, which reflects Monica's tumultuous life."
        }
      ]
    },
    {
      "@context": "http://iiif.io/api/presentation/2/context.json",
