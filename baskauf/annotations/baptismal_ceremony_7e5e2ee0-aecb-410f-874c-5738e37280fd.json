        }
      },
      "resource": [
        {
          "@type": "dctypes:Text",
          "format": "text/plain",
          "chars": "With the baptism occurring during Easter, the time when the liturgical color is white, St. Ambrose dons white robes with an intricate gold cloak."
        }
      ]
    },
    {
      "@context": "http://iiif.io/api/presentation/2/context.json",
      "@id": "http://7e5e2ee0-aecb-410f-874c-5738e37280fd/annotation/6",
