        }
      },
      "resource": [
        {
          "@type": "dctypes:Text",
          "format": "text/plain",
          "chars": "St. Monica appears here to accompany St. Augustine, her son. Monica greatly influenced Augustine's religious experience, hence her presence at his baptism. "
        }
      ]
    },
    {
      "@context": "http://iiif.io/api/presentation/2/context.json",
      "@id": "http://7e5e2ee0-aecb-410f-874c-5738e37280fd/annotation/8",
