        }
      },
      "resource": [
        {
          "@type": "dctypes:Text",
          "format": "text/plain",
          "chars": "Jesus, like Augustine and Ambrose, wears white. This may symbolize the innocence and purity of the Christ child, similar to the purity one receives after their baptism. "
        }
      ]
    },
    {
      "@context": "http://iiif.io/api/presentation/2/context.json",
