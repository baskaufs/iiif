      "resource": [
        {
          "@type": "dctypes:Text",
          "format": "text/plain",
          "chars": "St. Ambrose wears his full regalia as he presides over St. Augustine's baptism. "
        }
      ]
    },
    {
      "@context": "http://iiif.io/api/presentation/2/context.json",
