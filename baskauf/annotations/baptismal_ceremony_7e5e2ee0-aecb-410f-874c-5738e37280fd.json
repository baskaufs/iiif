      "resource": [
        {
          "@type": "dctypes:Text",
          "format": "text/plain",
          "chars": "Jesus appears as a child, possibly alluding to a well-known story of St. Augustine meeting a child on a beach in Jacobus de Voragine's Golden Legend, a late Medieval text."
        }
      ]
    },
    {
      "@context": "http://iiif.io/api/presentation/2/context.json",
