        "within": {
          "@id": "http://8899a42f-c4bb-4682-b073-6aea961b5b4d",
          "@type": "sc:Manifest"
        }
      },
      "resource": [
        {
          "@type": "dctypes:Text",
          "format": "text/plain",
          "chars": "He wears a white cloth to represent the purity of life after baptism: past sins are forgotten, and the baptized person is now born again without sin."
        }
      ]
    },
    {
      "@context": "http://iiif.io/api/presentation/2/context.json",
